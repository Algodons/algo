# Frontend Development Environment
# Next.js environment variables must be prefixed with NEXT_PUBLIC_ to be exposed to the browser

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:4000
NEXT_PUBLIC_WS_URL=ws://localhost:4000
NEXT_PUBLIC_BACKEND_URL=http://localhost:4000

# Copilot SaaS Configuration
NEXT_PUBLIC_COPILOT_API_URL=https://api-dev.copilot.example.com
NEXT_PUBLIC_COPILOT_ENABLED=true
NEXT_PUBLIC_COPILOT_WORKSPACE_ID=dev_workspace

# AI/ML Features
NEXT_PUBLIC_AI_FEATURES_ENABLED=true
NEXT_PUBLIC_ML_MODELS_ENABLED=true

# Environment
NEXT_PUBLIC_ENVIRONMENT=development
NODE_ENV=development

# Auth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=dev-nextauth-secret-key-change-in-production

# Feature Flags
NEXT_PUBLIC_ENABLE_COLLABORATION=true
NEXT_PUBLIC_ENABLE_GIT_INTEGRATION=true
NEXT_PUBLIC_ENABLE_DEBUG=true

# Analytics (Dev - disabled)
NEXT_PUBLIC_ANALYTICS_ENABLED=false

# Stripe (Test/Dev)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_key

# WebSocket Configuration
NEXT_PUBLIC_WS_RECONNECT_ATTEMPTS=5
NEXT_PUBLIC_WS_RECONNECT_DELAY=1000

# Development Settings
NEXT_PUBLIC_DEBUG_MODE=true
NEXT_PUBLIC_VERBOSE_LOGGING=true
